        "kpActions": "readonly",
        "kpErrors": "readonly",
        "kpxc": "readonly",
        "kpxcAssert": "readonly",
        "kpxcBanner": "readonly",
        "kpxcCustomLoginFieldsBanner": "readonly",
        "kpxcEvent": "readonly",
        "kpxcFields": "readonly",
        "kpxcFill": "readonly",
